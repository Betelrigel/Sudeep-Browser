{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" } 
      // You can change the runtime to python3.9, python3.10, python3.12 etc.
      // Make sure it matches what you used for development.
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py/$1"
    }
  ]
}